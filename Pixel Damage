#include<bits/stdc++.h>
using namespace std;
#define endl '\n'
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        long long h,w,x,y,k;
        cin>>h>>w>>x>>y>>k;
        long long p = sqrt((w-x)*(w-x)+(h-y)*(h-y));
        if(p < k) cout<<1<<endl;
        else cout<<0<<endl;
        
    }
    return 0;
}
